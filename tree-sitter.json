{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ptuscript",
      "camelcase": "PTUScript",
      "title": "PTUScript",
      "scope": "source.ptu",
      "file-types": [
        "ptu"
      ],
      "injection-regex": "^ptuscript$",
      "class-name": "TreeSitterPtuscript",
      "highlights": "queries/ptuscript/highlights.scm",
      "injections": "queries/ptuscript/injections.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for the PTUScript language from RTRT",
    "authors": [
      {
        "name": "Pierre Mollard",
        "email": "pierre.mollard@proton.me",
        "url": "https://pierremollard.com/"
      }
    ],
    "links": {
      "repository": "https://githubcom/Pierre-Mollard/tree-sitter-ptuscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
